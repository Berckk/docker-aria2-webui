server {
  listen 5555;
  listen [::]:5550;

  server_name _;

  location / {
    root /opt/aria2-webui;
  }
  location /data {
    alias /opt/data;
    autoindex on;
    fancyindex on;  
  }
  location /jsonrpc {
    proxy_pass http://localhost:6800/jsonrpc;
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection $connection_upgrade;
    proxy_read_timeout 86400;
  }
}
